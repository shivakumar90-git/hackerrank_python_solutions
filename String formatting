def print_formatted(number):
    w  = len(str(bin(number)).replace('0b',''))
    print("\n".join([f"{str(i).rjust(w,' ')} {oct(i)[2:].rjust(w,' ')} {hex(i)[2:].upper().rjust(w,' ')} {bin(i)[2:].rjust(w,' ')}" for i in range(1,number+1)])) 
    
if __name__ == '__main__':
    n = int(input())
    print_formatted(n)
